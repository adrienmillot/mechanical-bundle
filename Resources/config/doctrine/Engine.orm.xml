<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="amillot\MechanicalBundle\Entity\Engine" table="engines">
        <one-to-many field="engineParameters" target-entity="amillot\MechanicalBundle\Entity\EngineParameter" mapped-by="engine" />
        <one-to-many field="motors" target-entity="amillot\MechanicalBundle\Entity\Motor" mapped-by="engine" />
    </entity>

</doctrine-mapping>
